{
	"name": "mover",
	"properties": {
		"activities": [
			{
				"name": "Get Metadata1",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "datasourcefolder",
						"type": "DatasetReference",
						"parameters": {
							"contenedor": {
								"value": "@pipeline().parameters.sourcestore_location",
								"type": "Expression"
							},
							"directorio": {
								"value": "@pipeline().parameters.sourcestore_directory",
								"type": "Expression"
							}
						}
					},
					"fieldList": [
						"childItems"
					],
					"storeSettings": {
						"type": "AzureBlobStorageReadSettings",
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "BinaryReadSettings"
					}
				}
			}
		],
		"parameters": {
			"sourcestore_location": {
				"type": "string",
				"defaultValue": "ejercicio3"
			},
			"sourcestore_directory": {
				"type": "string",
				"defaultValue": "https://stalmacens.blob.core.windows.net"
			},
			"destinationstore_location": {
				"type": "string",
				"defaultValue": "https://stalmacens.blob.core.windows.net"
			},
			"destinationstore_directory": {
				"type": "string",
				"defaultValue": "ejercicio4"
			}
		},
		"annotations": []
	}
}